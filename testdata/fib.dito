# calculate by formuala.
cfibo := func(n) -> ((1 + r5)**n - (1 - r5)**n) / (r5 * 2**n)
r5    := sqrt(5)

# calculate by recursion.
fibo := func(n) -> (n if n < 2 else fibo(n-1) + fibo(n-2))

print(cfibo(50.0))
