# Dito hello world example.
print("Hello, World")

# Hello world example using a Lambda function.
greet = func(name) -> ("Hello, " + name)
print(greet("Dito!"))

# Hello to the whole crew function.
func hello_people(names) {
    for name in names {
        if name == names[-1] {
            print("And... Hello", name)
        } else {
            print("Hello", name)
        }
    }
}
# Hello to Tom Dick and Sally
hello_people(["Tom", "Dick", "Sally"])