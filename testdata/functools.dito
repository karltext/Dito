

first  = func(x)    -> x[0]
last   = func(x)    -> x[-1]
append = func(x, y) -> x + [y]
# pop    = func(x)    -> [x[0], x[:1]] # slices not implemented

func extend(x, y) {
    for i in y {
        x += y
    }
    return x
}

func reverse(array) {
    top = len(array) - 1
    bottom = 0
    for top > bottom {
        tmp = [array[bottom], array[top]]
        array[top] = tmp[0]
        array[bottom] = tmp[1]
        top -= 1
        bottom += 1
    }
}

